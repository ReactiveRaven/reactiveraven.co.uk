<VirtualHost *:80>
	DocumentRoot /var/data/reactiveraven/staging/site/root/web
	ServerName staging.reactiveraven.co.uk
	ServerAlias dev.reactiveraven.co.uk

	CustomLog /var/data/reactiveraven/staging/site/logs/apache/access combined
	LogLevel info
	ErrorLog /var/data/reactiveraven/staging/site/logs/apache/error

	php_flag log_errors on
	php_flag display_errors on
	php_value error_reporting 32767
	php_value error_log /var/data/reactiveraven/staging/site/logs/php/error
</VirtualHost>
