<VirtualHost *:80>
	DocumentRoot /var/data/reactiveraven/site/root/public
	ServerName www.reactiveraven.co.uk
	ServerAlias reactiveraven.co.uk

	CustomLog /var/data/reactiveraven/site/logs/apache/access combined
	LogLevel info
	ErrorLog /var/data/reactiveraven/site/logs/apache/error

	php_flag log_errors on
	php_flag display_errors on
	php_value error_reporting 32767
	php_value error_log /var/data/reactiveraven/site/logs/php/error
</VirtualHost>
